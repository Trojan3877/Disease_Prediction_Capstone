# ============================================================
# PYTHON & ML SYSTEM .gitignore (Optimized for RAG/LLM Projects)
# ============================================================

# -----------------------------
# PYTHON FILES / BUILD ARTIFACTS
# -----------------------------
__pycache__/
*.py[cod]
*.pyo
*.pyd
*.so
*.egg-info/
dist/
build/
pip-wheel-metadata/

# -----------------------------
# VIRTUAL ENVIRONMENTS
# -----------------------------
.env
.venv
venv/
ENV/
env/
*.virtualenv

# -----------------------------
# JUPYTER NOTEBOOKS
# -----------------------------
.ipynb_checkpoints/
*.nbconvert.ipynb

# -----------------------------
# LOGS
# -----------------------------
logs/
*.log

# -----------------------------
# MODEL ARTIFACTS
# -----------------------------
*.pkl
*.joblib
*.pt
*.pth
*.onnx
*.h5
*.model
src/models/*.pkl
src/models/*.joblib
src/models/*.json
src/models/*.txt

# -----------------------------
# RAG / VECTOR STORE ARTIFACTS
# -----------------------------
faiss_index/
*.faiss
*.index

# -----------------------------
# SECRETS / ENVIRONMENT CONFIG
# -----------------------------
.env
.env.*
*.key
*.secret
api_keys.json

# -----------------------------
# DATA FILES (RAW & PROCESSED)
# -----------------------------
data/raw/*
data/processed/*
!data/raw/.gitkeep
!data/processed/.gitkeep

# -----------------------------
# CACHE / TEMP FILES
# -----------------------------
*.cache
*.tmp
tmp/
.cache/

# -----------------------------
# DOCKER
# -----------------------------
docker-build/
docker-cache/
*.tar

# -----------------------------
# VS CODE
# -----------------------------
.vscode/
.history/
*.code-workspace

# -----------------------------
# TESTS / COVERAGE
# -----------------------------
.coverage
coverage.xml
pytest_cache/
htmlcov/

# -----------------------------
# MISC
# -----------------------------
.DS_Store
Thumbs.db
